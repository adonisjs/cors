{
  "name": "@adonisjs/cors",
  "version": "2.0.0-0",
  "description": "CORS middleware for AdonisJS HTTP server",
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build/src",
    "build/stubs",
    "build/providers",
    "build/index.d.ts",
    "build/index.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js",
    "./cors_middleware": "./build/src/cors_middleware.js",
    "./cors_provider": "./build/providers/cors_provider.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run vscode:test",
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "compile": "npm run lint && npm run clean && tsc && npm run copy:templates",
    "build": "npm run compile",
    "release": "np",
    "version": "npm run build",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "sync-labels": "github-label-sync --labels .github/labels.json adonisjs/cors",
    "vscode:test": "node --loader=ts-node/esm bin/test.ts"
  },
  "keywords": [
    "adonisjs",
    "cors-middleware",
    "cors",
    "adonisjs-core"
  ],
  "author": "adonisjs,virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/core": "^6.0.1-0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@japa/assert": "^1.4.1",
    "@japa/run-failed-tests": "^1.1.1",
    "@japa/runner": "^2.5.0",
    "@japa/spec-reporter": "^1.3.3",
    "@swc/core": "^1.3.35",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "c8": "^7.13.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^3.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "github-label-sync": "^2.2.0",
    "husky": "^8.0.3",
    "np": "^7.6.1",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.0.1-0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/cors.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/cors/issues"
  },
  "homepage": "https://github.com/adonisjs/cors#readme",
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      ".yalc/**"
    ]
  },
  "adonisjs": {
    "stubs": "./build/stubs",
    "providers": [
      "@adonisjs/cors/cors_provider"
    ]
  }
}
